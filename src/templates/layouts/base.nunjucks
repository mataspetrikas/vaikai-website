<!doctype html>

<html class="no-js" lang="en">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- =Robots & SEO -->
    <title>{{ page.title }} | {{ site.title }}</title>
    <meta name="description" content="{{ page.description }}" />
    <meta name="keywords" content="{{ (site.keywords|merge(page.keywords)) }}" />
    <meta name="author" content="{{ site.author }}" />
    <meta name="web-author" content="{{ site.webAuthor }}" />
    <link rel="canonical" href="{{ page.url }}" />

    <!-- =Scripts-preload -->
    {% block head_scripts %}
      <script src="{{ 'js/vendor/modernizr.js'|url }}"></script>
      <script>
      function loadJS(u){var r=document.getElementsByTagName("script")[0],s=document.createElement("script");s.src=u;r.parentNode.insertBefore(s,r);}

      if(!window.HTMLPictureElement || !('sizes' in document.createElement('img'))){
        loadJS("{{ 'js/vendor/ls.respimg.js'|url }}");
      }
    </script>
    <script src="{{ 'js/vendor/ls.parent-fit.js'|url }}"></script>
    <script src="{{ 'js/vendor/ls.bgset.js'|url }}"></script>
    <script src="{{ 'js/vendor/lazysizes.js'|url }}"></script>
    <script>
      window.lazySizesConfig = window.lazySizesConfig || {};
      window.lazySizesConfig.customMedia = {
        "--s": "(max-width: 540px)",
        "s--": "(min-width: 540px)",
        "--sm": "(max-width: 700px)",
        "sm--": "(min-width: 700px)",
        "--m": "(max-width: 920px)",
        "m--": "(min-width: 920px)",
        "--l": "(max-width: 1200px)",
        "l--": "(min-width: 1200px)"
      };
    </script>
    {% endblock %}

    <!-- =Stylesheets -->
    {% block head_styles %}
      <link rel="stylesheet" href="css/main.css" />
      <!--[if lt IE 9]>
        <link rel="stylesheet" media="screen" href="css/screen-old-ie9.css" />
      <![endif]-->

      <link rel="stylesheet" media="print" href="css/print.css" />
      <!--[if lt IE 9]>
        <link rel="stylesheet" media="print" href="css/print-old-ie9.css" />
      <![endif]-->
    {% endblock %}

    <!-- =Icons-->
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
    <!-- Place favicon.ico in the root directory -->

    <!-- =Third-party-meta -->
    {% block third_party_meta %}
      {% include "partials/facebook/meta.nunjucks" %}
      {% include "partials/twitter/card.nunjucks" %}
    {% endblock %}
  </head>

  <body>
    <div class="root">
      <!--[if lt IE 8]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
      <![endif]-->

      {% block body %}
        <!-- =Header -->
        <header class="page-header" role="banner">
          {% block body_header %}
            {% include "partials/navbar.nunjucks" %}
          {% endblock %}
        </header>

        <!-- =Main -->
        <main class="page-main menu-push" role="main">
          {% block body_main %}{% endblock %}
        </main>

        <!-- =Footer -->
        <footer class="page-footer menu-push" role="contentinfo">
          {% block body_footer %}
            {% include "partials/footer.nunjucks" %}
          {% endblock %}
        </footer>
      {% endblock %}
    </div>

    <!-- =Body-end -->
    {% block body_end %}
      <!-- =jQuery -->
      {% set jQueryCdnFile = 'jquery-1.11.3.js' if app.environment == 'dev' else 'jquery-1.11.3.min.js' %}
      <script src="https://code.jquery.com/{{ jQueryCdnFile }}"></script>
      <script>window.jQuery || document.write('<script src="./js/vendor/jquery.js"><\/script>');</script>

      {% if app.watch %}
        <!-- =Scripts-watch -->
        <script src="{{ app.servers.livereload.url }}"></script>
        <script src="{{ app.servers.webpackDev.url }}"></script>
      {% endif %}

      <!-- =Polyfills -->
      <!--[if lte IE 8]>
        <script src="{{ 'js/vendor/polyfills.js'|webpackurl }}"></script>
      <![endif]-->

      <!-- =Scripts-custom -->
      <script src="{{ 'js/main.js'|webpackurl }}"></script>
    {% endblock %}

    {% block third_party_scripts %}
      <!-- =Third-party scripts -->
      {% include 'partials/celery/embed.nunjucks' %}
      {% include 'partials/google/analytics.nunjucks' %}
      {% include 'partials/facebook/pixel.nunjucks' %}
    {% endblock %}
  </body>
</html>