{% extends "layouts/page-info.nunjucks" %}

{% block head_styles %}
  {{ super() }}
  <link rel="stylesheet" href="{{ 'css/company.css'|url }}"></link>
{% endblock %}

{% block body_main %}

  {# include page sections #}
  {% for section in sections %}
    {% include section.template ~ '.nunjucks' %}
  {% endfor %}

{% endblock %}

{% block body_end %}
  {{ super() }}
  <script src="{{ 'js/company.js'|webpackurl }}"></script>
{% endblock %}